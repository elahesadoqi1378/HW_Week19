@{
    ViewData["Title"] = "Transaction History";
}

<h2>Your Transactions</h2>

@if (Model != null && Model.Any())
{
        <ul>
        @foreach (var transaction in Model)
        {
                    <li>@transaction.Description</li>
        }
        </ul>
}
else
{
        <p>No transactions found.</p>
}
